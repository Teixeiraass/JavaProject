
import java.util.*;

public class VontadeDeMeMatar {

	public static void main(String[] args) {
			
		Scanner ler = new Scanner(System.in);
		
		int num;
		System.out.println("Digite um numero");
		num = ler.nextInt();
		int cont = 1;
		
		for(int j = 1; j <= 2; j++) {
			for(int i = num;i <= 1; i-- ) {
				for(int a = cont; cont <= num; a++) {
					System.out.print("*");
				}
				System.out.print("\n");
				cont++;
			}
		}
	}

}
