package exercicio;

import java.util.*;

public class Faculdade {
	
	public static void main(String[] args) {
		
		Scanner ler = new Scanner(System.in);
		
		Random gerador = new Random((10 - 0 + 1) + 0);
		int num, numR;
		
		System.out.println("Digite o numero que a maquina tem que advinhar");
		num = ler.nextInt();
		numR = gerador.nextInt();
		
		System.out.println(numR);
	}
	
}
